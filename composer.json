{
    "name": "coinbar/coinbar-pay-m2-plg",
    "description": "Magento 2 Plug-in for CoinbarPay",
    "type": "magento2-module",
    "version": "1.0.3",
    "license": "LGPL-3.0",
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "Coinbar\\CoinbarPay\\": ""
        }
    },
    "authors": [
        {
            "name": "Coinbar SPA",
            "email": "info@coinbar.io"
        },
        {
            "name": "Adamantic",
            "email": "info@coinbar.io"
        }
        
    ],
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "coinbar/coinbar-pay-php-sdk": "^1.0.0"
    },
    "minimum-stability": "stable",
    "repositories": [
        { "type": "vcs", "url": "https://github.com/CoinbarSPA/coinbar-pay-php-sdk" },
        { "type": "vcs", "url": "https://github.com/CoinbarSPA/crypto-payments-php" }
    ]
}
